        <dataField fieldName="IDPaciente" aliasFieldName="IDPacientePass" />
        <dataField fieldName="IDSeguro" columns="15" />
        <dataField fieldName="Fecha" columns="50" />
        <dataField fieldName="DatosClinicos" />
        <dataField fieldName="Estado" />
