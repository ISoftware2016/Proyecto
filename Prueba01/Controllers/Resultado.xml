        <dataField fieldName="NameAnalisis" columns="50" />
        <dataField fieldName="Valor" columns="15" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Resultado">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
